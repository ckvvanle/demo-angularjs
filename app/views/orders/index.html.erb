<div class="container">
  <div class="row">
    <div class="col-md-12">
      <h4><%= t ".my_order" %></h4>
      <div class="table-responsive">
        <table id="mytable" class="table table-bordred table-striped">
          <thead>
            <th><input type="checkbox" id="checkall" /></th>
            <th><%= t ".Name" %></th>
            <th><%= t ".Price" %></th>
            <th><%= t ".Order" %></th>
            <th><%= t ".Email" %></th>
            <th><%= t ".Date" %></th>
            <th><%= t ".Status" %></th>
            <th><%= t ".Delete" %></th>
          </thead>
          <tbody>
            <tr>
              <td><input type="checkbox" class="checkthis" /></td>
              <td><%= t ".Mohsin" %></td>
              <td><%= t ".price_data" %></td>
              <td><%= t ".order_bill" %></td>
              <td><%= t "t_mail" %></td>
              <td><%= day_format %></td>
              <td class="text-primary"><%= t ".wrating" %></td>
              <td><p data-placement="top" data-toggle="tooltip" title="Delete"><button class="btn btn-danger btn-xs" data-title="Delete" data-toggle="modal" data-target="#delete" ><span class="glyphicon glyphicon-trash"></span></button></p></td>
            </tr>
          </tbody>
        </table>
        <div class="clearfix"></div>
        <div>
          <ul class="pagination pull-right">
          </ul>
        </div>
      </div>
    </div>
  </div>
</div>
<div class="modal fade" id="delete" tabindex="-1" role="dialog" aria-labelledby="edit" aria-hidden="true">
  <div class="modal-dialog">
    <div class="modal-content">
      <div class="modal-header">
        <button type="button" class="close" data-dismiss="modal" aria-hidden="true">
          <span class="glyphicon glyphicon-remove" aria-hidden="true"></span>
        </button>
        <h4 class="modal-title custom_align" id="Heading"><%= t ".delete_his" %></h4>
      </div>
      <div class="modal-body">
        <div class="alert alert-danger">
          <span class="glyphicon glyphicon-warning-sign"></span><%= t ".you_sure" %>
        </div>
      </div>
      <div class="modal-footer ">
        <button type="button" class="btn btn-success" >
          <span class="glyphicon glyphicon-ok-sign"></span><%= t ".yes" %>
        </button>
        <button type="button" class="btn btn-default" data-dismiss="modal">
          <span class="glyphicon glyphicon-remove"></span><%= t ".no" %>
        </button>
      </div>
    </div>
  </div>
</div>
